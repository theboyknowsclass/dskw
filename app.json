{
  "expo": {
    "name": "STR8N",
    "slug": "str8n",
    "scheme": "str8n-scheme",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon-with-border-1024.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-with-border-1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/orbitron/Orbitron_400Regular.ttf",
            "node_modules/@expo-google-fonts/orbitron/Orbitron_500Medium.ttf",
            "node_modules/@expo-google-fonts/orbitron/Orbitron_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/orbitron/Orbitron_700Bold.ttf",
            "node_modules/@expo-google-fonts/orbitron/Orbitron_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/orbitron/Orbitron_900Black.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/icon-with-border-1024.png",
          "dark": {
            "image": "./assets/icon-with-border-1024.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ineffable.str8n",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select and upload images.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save photos to your library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "light": "./assets/icon-with-border-1024.png",
        "dark": "./assets/icon-with-border-inverse-1024.png",
        "tinted": "./assets/icon-with-border-grayscale-1024.png"
      }
    },
    "android": {
      "package": "com.ineffable.str8n",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "experiments": {
      "baseUrl": "/str8n"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "69fa859a-06fa-4151-b1b8-b8b2adb7476f"
      }
    },
    "owner": "theboyknowsclass"
  }
}
