{
  "name": "str8n",
  "version": "1.0.0",
  "main": "index",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo prebuild --clean && npx expo run:android",
    "ios": "npx expo prebuild --clean && npx expo run:ios",
    "web": "npx expo start --web",
    "build:web": "npx expo export --platform web",
    "build:android": "npx expo prebuild --clean --platform android",
    "build:ios": "npx expo prebuild --clean --platform ios",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "expo:update": "npx expo install --fix"
  },
  "dependencies": {
    "@expo-google-fonts/orbitron": "^0.2.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@techstark/opencv-js": "^4.10.0-release.1",
    "expo": "^53.0.9",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.10",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "file-saver": "^2.0.5",
    "module-alias": "^2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-fast-opencv": "^0.4.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "zustand": "^5.0.3",
    "typescript": "~5.8.3",
    "@types/react": "~19.0.10",
    "@shopify/react-native-skia": "v2.0.0-next.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.23.0",
    "@types/file-saver": "^2.0.7",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "@techstark/opencv-js",
          "file-saver",
          "module-alias"
        ],
        "listUnknownPackages": true
      }
    }
  },
  "private": true
}
